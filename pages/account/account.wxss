.first-fixed {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 100;
  padding-bottom: constant(34rpx);
  background: rgba(0, 0, 0, 0.7);
  background-size: 100%;
}
.image-guide{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
}
